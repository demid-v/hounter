{
  "name": "hounter",
  "version": "0.1.0",
  "description": "Houter - House Hunter UI Design (Community)",
  "homepage": "https://demid-v.github.io/hounter",
  "scripts": {
    "dev": "npx tailwindcss -i ./base.css -o ./dist/base.css --watch",
    "start": "npx tailwindcss -i ./base.css -o ./dist/base.css",
    "start:init": "npm ci && npm run start && open-cli ./index.html",
