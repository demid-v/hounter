    "landing page",
    "homepage",
    "static"
  ],
  "author": "Demid Vasilyev <vasilyev.demid.e@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/demid-v/hounter/issues"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "open-cli": "^7.1.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4"
  }
}
